{
    "editor.formatOnSave": true,
    "[typescript]": {
        "editor.formatOnSave": true
    },
    "workbench.colorCustomizations": {
        "editor.stackFrameHighlightBackground": "#dc6873"
    },

    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true,
    },

}
